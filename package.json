{
  "name": "redirect.center",
  "version": "2.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon -w src",
    "start": "node src",
    "test": "npm run eslint && PORT=3001 nyc mocha",
    "eslint": "eslint src/",
    "coverage": "PORT=3001 nyc --reporter html npm test",
    "codacy-coverage": "PORT=3001 nyc --reporter lcovonly npm test && cat ./coverage/lcov.info | codacy-coverage && rm -rf ./coverage",
    "pm2-start": "pm2 start --name redirect-center src/index.js",
    "pm2-status": "pm2 status",
    "pm2-log": "pm2 log --raw redirect-center",
    "pm2-stop": "pm2 stop redirect-center && pm2 delete redirect-center"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.spec.js",
      "src/**/*.mock.js"
    ],
    "all": true
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "cuid": "^1.3.8",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "nyc": "^11.1.0",
    "parse-domain": "^1.1.0",
    "pm2": "^2.6.1",
    "redis": "^2.7.1",
    "vhost": "^3.0.2",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "chai-json-schema": "^1.4.0",
    "codacy-coverage": "^2.0.2",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "node-mocks-http": "^1.6.1",
    "nodemon": "^1.11.0",
    "sinon": "^2.2.0"
  },
  "author": "",
  "license": "ISC"
}
